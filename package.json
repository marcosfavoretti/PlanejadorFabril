{
  "name": "planejamento-ethos",
  "version": "0.0.1",
  "description": "",
  "author": "Marcos Favoretti Jr.",
  "private": true,
  "license": "UNLICENSED",
  "engines": {
    "node": ">=22.12.0"
  },
  "scripts": {
    "start:dev:importa": "nest start importa-pedido --watch --debug",
    "start:dev:worker": "nest start planeja-worker --watch --debug",
    "start:dev:api": "nest start --watch",
    "start:dev:env": "concurrently \"npm:start:dev:api\" \"npm:start:dev:worker\" \"npm:start:dev:pedido\" --prefix-colors cyan.bold,yellow.bold,green.bold",
    "dev-api": "nest start --watch planejamento-ethos",
    "build": "nest build importa-pedido planeja-worker planejamento-ethos",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/apps/planejamento-ethos/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./apps/planejamento-ethos/test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs/bull": "^11.0.3",
    "@nestjs/bullmq": "^11.0.3",
    "@nestjs/common": "^11.0.1",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.0.1",
    "@nestjs/microservices": "^11.1.6",
    "@nestjs/platform-express": "^11.0.1",
    "@nestjs/schedule": "^6.0.0",
    "@nestjs/swagger": "^11.2.0",
    "@nestjs/terminus": "^11.0.0",
    "@nestjs/typeorm": "^11.0.0",
    "@types/axios": "^0.9.36",
    "axios": "^1.11.0",
    "bull": "^4.16.5",
    "bullmq": "^5.58.5",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "date-fns": "^4.1.0",
    "dotenv": "^16.5.0",
    "ioredis": "^5.7.0",
    "jsonwebtoken": "^9.0.2",
    "mailgen": "^2.0.29",
    "mssql": "^11.0.1",
    "oracledb": "^6.9.0",
    "planejamento-ethos": "file:",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "sqlite3": "^5.1.7",
    "typeorm": "^0.3.24",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.18.0",
    "@nestjs/cli": "^11.0.0",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.1",
    "@swc/cli": "^0.6.0",
    "@swc/core": "^1.10.7",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.7",
    "@types/supertest": "^6.0.2",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.2",
    "globals": "^15.14.0",
    "jest": "^29.7.0",
    "prettier": "^3.4.2",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.20.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageReporters": [
      "text",
      "lcov",
      "json-summary"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/dist/",
      ".*\\.dto\\..*",
      ".*\\.mapper\\..*",
      ".*\\.module\\..*",
      ".*\\.model\\..*",
      ".*\\.modelOrm\\..*",
      ".*\\.enum\\..*",
      ".*\\.abstract\\..*",
      ".*\\.exception\\..*",
      ".*\\.export\\..*"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverage": true,
    "moduleNameMapper": {
      "^@config/(.*)$": "<rootDir>src/config/$1",
      "^@infra/(.*)$": "<rootDir>src/infra/$1",
      "^@lib/lib(|/.*)$": "<rootDir>/libs/lib/src/$1",
      "^src/(.*)$": "<rootDir>src/$1",
      "^test/(.*)$": "<rootDir>test/$1"
    },
    "coverageDirectory": "<rootDir>/test/coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/apps/",
      "<rootDir>/libs/"
    ]
  }
}
