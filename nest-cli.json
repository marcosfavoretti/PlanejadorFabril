{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "compilerOptions": {
    "deleteOutDir": true,
     "assets": [
      {
        "include": "@files/*",
        "outDir": "dist/modules/@files"
      },
      {
        "include": "@sql/*",
        "outDir": "dist/modules/@sql"
      }
    ]
  }
    
}
