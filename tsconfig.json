{
  "compilerOptions": {
    "module": "commonjs",
    "declaration": true,
    "removeComments": true,
    "paths": {
      "@dto/*": [
        "libs/lib/src/dtos/*"
      ],
      "@files/*": [
        "src/modules/@sql/*"
      ],
      "@libs/lib/*": [
        "libs/lib/src/*"
      ],
      "@sql/*": [
        "src/modules/@files/*"
      ]
    },
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "target": "ES2021",
    "sourceMap": true,
    "outDir": "./dist",
    "baseUrl": "./",
    "incremental": true,
    "skipLibCheck": true,
    "strictNullChecks": true,
    "forceConsistentCasingInFileNames": true,
    "noImplicitAny": false,
    "strictBindCallApply": false,
    "noFallthroughCasesInSwitch": false
  }
}